<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
<style name="SwitchTheme" parent="@android:style/TextAppearance.Small">
    <item name="android:textColor">@color/switch_text_selector</item>
</style>

    <declare-styleable name="GridImageViewGroup">
        <attr name="childVerticalSpce" format="dimension"/>
        <attr name="childHorizontalSpace" format="dimension"/>
        <attr name="columnNum" format="integer"/>
    </declare-styleable>
</resources>
